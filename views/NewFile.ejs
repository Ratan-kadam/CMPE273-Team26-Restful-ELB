    <script>
    $(document).ready(function(){
  $("#AddConfig").click(function () {
     $("#mytable").each(function () {
         var tds = '<tr>';
         jQuery.each($('tr:last', this), function () {
             tds += this.innerHTML;
         });
         tds += '</tr>';
         if ($('tbody', this).length > 0) {
             $('tbody', this).append(tds);
         } else {
             $(this).append(tds);
         }
     });
});
	$("#RemoveConfig").click(function () {
     $("#mytable").each(function () {
		jQuery.each($('tr:last', this), function () {
             this.remove();
         });
	 });
	});

});
    </script>
